    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "serve": "^14.2.4",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "glob": "^11.0.4"
  },
  "engines": {
    "node": "20.10.0"
  }
}
