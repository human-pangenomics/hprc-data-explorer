{
  "name": "hprc-data-explorer",
  "version": "0.6.1",
  "private": true,
  "scripts": {
    "dev": "./scripts/dev.sh hprc-data-explorer && ./scripts/set-version.sh dev && next dev",
    "build:local": "./scripts/build.sh hprc-data-explorer local && ./scripts/set-version.sh && next build",
    "build:prod": "./scripts/build.sh hprc-data-explorer prod && ./scripts/set-version.sh && next build",
    "start": "npx serve out",
    "lint": "next lint --dir .",
    "check-format": "prettier --check .",
    "prepare": "husky",
    "test": "jest --runInBand",
    "test:e2e": "playwright test --trace retain-on-failure",
    "build-catalog": "esrun catalog/build/ts/build-catalog.ts",
    "build-catalog-source": "poetry run python catalog/build/py/build_sequencing_data.py && poetry run python catalog/build/py/build_assemblies.py && poetry run python catalog/build/py/build_annotations.py",
    "gen-schema": "poetry run ./catalog/schema/scripts/gen-schema.sh"
  },
  "dependencies": {
    "@databiosphere/findable-ui": "^46.1.3",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@next/mdx": "^14.2.33",
    "@observablehq/plot": "^0.6.17",
    "@tanstack/react-table": "^8.19.2",
    "@tanstack/react-virtual": "^3.13.12",
    "gray-matter": "^4.0.3",
    "isomorphic-dompurify": "^2.22.0",
    "next": "^14.2.33",
    "next-auth": "^4.24.13",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "1.8.9",
    "rehype-raw": "^7.0.0",
    "rehype-react": "^7.2.0",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "slugify": "^1.6.6",
    "unified": "^10.1.2",
    "uuid": "8.3.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@mui/types": "^7.4.0",
    "@next/eslint-plugin-next": "^14.2.33",
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/mdx": "^2.0.11",
    "@types/node": "20.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-table": "^7.7.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "babel-loader": "^9.2.1",
    "csv-parse": "^5.6.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.33",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jsdoc": "^50.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
